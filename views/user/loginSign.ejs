        <div class="tabs px-4">
          <button class="tab-button active" onclick="showTab('login')">
            Login
          </button>
          <button class="tab-button" onclick="showTab('signup')">
            Sign Up
          </button>
        </div>
        <div class="tab-content px-4" id="login">
          <!-- <h3 class="mb-3">Login</h3> -->
          <form class="needs-validation" action="/login" method="post" novalidate>
            <div class="mb-3">
              <label class="form-label" for="login-email">Username</label>
              <input
                class="form-control"
                type="text"
                id="login-email"
                name="username"
                required
              />
              <div class="invalid-feedback">Username required!</div>
            </div>

            <div class="mb-3">
              <label class="form-label" for="login-password">Password:</label>
              <input
                class="form-control"
                type="password"
                id="login-password"
                name="password"
                required
              />
              <div class="invalid-feedback">Password required</div>
            </div>

            <button type="submit"  class="col-md-3 btn btn-warning text-white ">Login</button>
            <p class="my-2 col-sm-10 offset-sm-3 offset-md-1 offset-xl-2">If you don't have an account ? &nbsp;<span class="purple fw-bold" onclick="showTab('signup')">Sign Up.</span></p>
          </form>
        </div>
        <div class="tab-content" id="signup" style="display: none">
          <!-- <h3 class="mb-3">Sign Up</h3> -->
          <form action="/user" method="post" class="needs-validation" novalidate>
            <div class="mb-3">
              <label class="form-label" for="signup-name">Username:</label>
              <input
                class="form-control"
                type="text"
                id="signup-name"
                name="username"
                required
              />
              <div class="invalid-feedback">Username required!</div>
            </div>
            <div class="mb-3">
              <label class="form-label" for="signup-email">Email:</label>
              <input
                class="form-control"
                type="email"
                id="signup-email"
                name="email"
                required
              />
              <div class="invalid-feedback">Email required!</div>
            </div>
            <div class="mb-3">
              <label class="form-label" for="signup-phone">Phone No:</label>
              <input
                class="form-control"
                type="tel"
                id="signup-phone"
                name="phone"
                required
              />
              <div class="invalid-feedback">Password must be strong!</div>
            </div>
            <div class="mb-3">
              <label class="form-label" for="signup-password">Password:</label>
              <input
                class="form-control"
                type="password"
                id="signup-password"
                name="password"
                required
              />
              <div class="invalid-feedback">Password must be strong!</div>
            </div>
            <button type="submit" class="col-md-3  btn btn-warning text-white">Sign Up</button>
            <p class="my-2  col-sm-10 offset-sm-3 offset-md-1 offset-xl-2">If you already have an account ? &nbsp;<span class="purple fw-bold" onclick="showTab('login')">Login</span></p>
          </form>
        </div>
