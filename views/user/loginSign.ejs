      .tab-body{
        background-image: url('/images/loginBg.png');
        height: 100vh;
        display: flex;
        flex-direction: column;
        justify-content: center;
        /* align-items: center; */
      }
      .container {
        display: flex;
        flex-direction: column;
        justify-content: center;
        margin: auto;
        width: 40% !important;
        border-radius: 8px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        overflow: hidden;
        background-color: rgba(255, 255, 255, 0.8);
        backdrop-filter: blur(0.8);
      }

      .tabs {
        display: flex;
      }

      .tab-button {
        flex: 1;
        padding: 10px;
        background: #eee;
        border: none;
        cursor: pointer;
        text-align: center;
      }

      .tab-button.active {
        background: #802cfc;
        color: #fff;
      }

      .tab-content {
        padding: 20px;
      }
      .purple{
        color:#802cfc;
        cursor: pointer;
      }
      .col-md-3{
        display: block!important;
        margin: auto;
      }
      @media (max-width:768px) {
        .container{
          width: 100% !important;
        }
        .tab-header{
          width: 80%;
        }
      }
    </style>
